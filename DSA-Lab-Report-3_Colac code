        "def sum_evens(scores):\n",
        "    \"\"\"Calculate sum of even numbers in a list.\"\"\"\n",
        "    total = 0\n",
        "    for num in scores:\n",
        "        if num % 2 == 0:\n",
        "            total += num\n",
        "    return total\n",
        "\n",
        "scores = [26, 49, 98, 87, 62, 75]\n",
        "print(\"Sum of even scores:\", sum_evens(scores))"
